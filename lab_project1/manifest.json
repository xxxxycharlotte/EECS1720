
  {
    "name": "Yuchen Xia lab projetct1",
    "description": "Build an Extension that can change the background color of the goole web page.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png"
      }
    },
    "icons": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
    }
  }
  